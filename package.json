{
  "name": "sample-webdriver-io",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "3.4.0",
    "@types/jest": "24.0.17",
    "@types/selenium-standalone": "6.15.0",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "@wdio/cli": "5.12.4",
    "@wdio/jasmine-framework": "5.12.1",
    "@wdio/local-runner": "5.12.4",
    "@wdio/selenium-standalone-service": "5.12.1",
    "@wdio/spec-reporter": "5.12.1",
    "acorn": "6.3.0",
    "chromedriver": "76.0.0",
    "eslint": "6.2.0",
    "eslint-config-xo": "0.26.0",
    "eslint-config-xo-typescript": "0.17.0",
    "eslint-plugin-jest": "22.15.1",
    "http-server": "0.11.1",
    "jest": "24.9.0",
    "lodash": "4.17.15",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "selenium-standalone": "6.16.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.5.3",
    "wdio-chromedriver-service": "5.0.2",
    "wdio-image-comparison-service": "1.5.1",
    "wdio-video-reporter": "1.4.4",
    "xo": "0.24.0"
  },
  "scripts": {
    "clean": "rimraf ./dest",
    "format": "prettier --write **/*.ts **/*.json",
    "lint": "xo",
    "report": "allure generate dest/wdio/allure-raw -o dest/wdio/allure-report",
    "serve": "http-server dest/wdio/allure-report -p 8080 -a 0.0.0.0",
    "test": "jest",
    "test:wdio": "wdio",
    "test:wdio:report": "./bin/test-and-report"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "xo-typescript"
    ]
  },
  "jest": {
    "clearMocks": true,
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/jest/**/*.test.ts"
    ]
  },
  "xo": {
    "space": true,
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
